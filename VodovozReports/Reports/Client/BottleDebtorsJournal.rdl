  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    counterparty.name,
    IFNULL(delivery_points.compiled_address, 'Самовывоз') AS delivery_point,
    IFNULL(
        (SELECT TRIM('\n' FROM GROUP_CONCAT(DISTINCT CONCAT('8', digits_number) SEPARATOR ';\n'))
        FROM phones
        WHERE delivery_point_id = orders.delivery_point_id),
        (SELECT TRIM('\n' FROM GROUP_CONCAT(DISTINCT CONCAT('8', digits_number) SEPARATOR ';\n'))
        FROM phones
        WHERE counterparty_id = orders.client_id)
    ) AS phones,
    (
        SELECT 
            SUM(CAST(bm.delivered AS SIGNED) - CAST(bm.returned AS SIGNED)) AS sum
        FROM 
            bottles_movement_operations AS bm
        WHERE 
            bm.counterparty_id = counterparty.id 
            AND (delivery_points.id = bm.delivery_point_id 
                OR (delivery_points.id IS NULL AND bm.delivery_point_id IS NULL AND bm.order_id IS NOT NULL)
                )  
    ) as bottle_debt,
    bottles_movement_operations.delivered as delivered_bottles
FROM
    orders 
    LEFT JOIN counterparty ON orders.client_id = counterparty.id 
    LEFT JOIN delivery_points ON orders.delivery_point_id = delivery_points.id 
    LEFT JOIN bottles_movement_operations ON orders.bottles_movement_operation_id = bottles_movement_operations.id
    LEFT JOIN phones AS client_phone ON client_phone.counterparty_id = counterparty.id
    LEFT JOIN phones AS dp_phone ON dp_phone.delivery_point_id = delivery_points.id
WHERE
    (@CounterpartyId = 0 OR orders.client_id = @CounterpartyId)
    AND (@AddressId = 0 OR orders.delivery_point_id = @AddressId)
    AND (@OPF = '' OR counterparty.person_type = @OPF)
    AND
    (
        @nomenclature_id = 0
        OR
        EXISTS 
        (
            SELECT
                nomenclature.id
            FROM
                order_items
                LEFT OUTER JOIN nomenclature ON order_items.nomenclature_id = nomenclature.id 
            WHERE
                order_items.order_id = orders.id 
                AND nomenclature.id = @nomenclature_id
        )
    )

    AND
    (
        @discount_reason_id = 0
        OR
        EXISTS 
        (
            SELECT
                discount_reasons.id 
            FROM
                order_items 
                LEFT OUTER JOIN discount_reasons ON order_items.discount_reason_id = discount_reasons.id 
            WHERE
                order_items.order_id = orders.id 
                AND discount_reasons.id = @discount_reason_id
        )
    )
    AND (@OrderBottlesFrom = '' OR bottles_movement_operations.delivered &gt;= @OrderBottlesFrom)
    AND (@OrderBottlesTo = '' OR bottles_movement_operations.delivered &lt;= @OrderBottlesTo)
    AND (@StartDate = '' OR orders.delivery_date &gt;= DATE(@StartDate))
    AND (@EndDate = '' OR orders.delivery_date &lt;= DATE(@EndDate))
    AND (@EndDate = '' OR @HideActiveCounterparty = '' OR NOT EXISTS 
    (
        SELECT
            *
        FROM
            orders AS order_from_another_dp
        WHERE
            order_from_another_dp.client_id = counterparty.id
            AND order_from_another_dp.order_status = 'Closed'
            AND order_from_another_dp.delivery_date &gt;= orders.delivery_date
            AND (
                (order_from_another_dp.self_delivery AND !orders.self_delivery)
                OR (!order_from_another_dp.self_delivery AND orders.self_delivery)
                OR  (order_from_another_dp.delivery_point_id != orders.delivery_point_id)
            )
    ))
    AND (@DebtBottlesFrom = '' OR EXISTS 
    (
        SELECT 
            SUM(CAST(bm.delivered AS SIGNED) - CAST(bm.returned AS SIGNED)) AS sum
        FROM 
            bottles_movement_operations AS bm
        WHERE 
            bm.counterparty_id = counterparty.id 
            AND (delivery_points.id = bm.delivery_point_id 
                    OR (delivery_points.id IS NULL AND bm.delivery_point_id IS NULL AND bm.order_id IS NOT NULL)
                )
        HAVING sum &gt;= @DebtBottlesFrom         
    ))
    AND (@DebtBottlesTo = '' OR EXISTS 
    (
        SELECT 
            SUM(CAST(bm.delivered AS SIGNED) - CAST(bm.returned AS SIGNED)) AS sum
        FROM 
            bottles_movement_operations AS bm
        WHERE 
            bm.counterparty_id = counterparty.id 
            AND (delivery_points.id = bm.delivery_point_id 
                    OR (delivery_points.id IS NULL AND bm.delivery_point_id IS NULL AND bm.order_id IS NOT NULL)
                )
        HAVING sum &lt;= @DebtBottlesTo         
    ))
    AND orders.id = 
    (
        SELECT
            ord.id
        FROM
            orders AS ord
        WHERE
            ord.client_id = counterparty.id
            AND (
                (
                    ord.self_delivery = TRUE 
                    AND orders.delivery_point_id IS NULL
                ) 
                OR ord.delivery_point_id = delivery_points.id
            ) 
            AND ord.order_status = 'Closed'
        ORDER BY
            ord.id DESC LIMIT 1
    )
    AND (
        (
        @HideWithOneOrder = '' 
        OR          
            (
                1 &lt;&gt; (
                    SELECT 
                        COUNT(DISTINCT o2.id) 
                    FROM 
                        orders o2
                        LEFT JOIN order_items oi2 ON oi2.order_id = o2.id
                        LEFT JOIN nomenclature n2 ON n2.id = oi2.nomenclature_id
                    WHERE 
                        o2.client_id = counterparty.id
                        AND n2.category = 'water'
                )
            )
        )
    )
    AND (
    		(
    			delivery_points.compiled_address LIKE (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, CONCAT('%',@SearchString1,'%'), delivery_points.compiled_address))
    			OR
    			counterparty.name  LIKE (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, CONCAT('%',@SearchString1,'%'), counterparty.name))
    			OR
    			delivery_points.id  = (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, @SearchString1, delivery_points.id))
    			OR
    			counterparty.id = (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, @SearchString1, counterparty.id))
			)
			AND
			(
    			delivery_points.compiled_address LIKE (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, CONCAT('%',@SearchString2,'%'), delivery_points.compiled_address))
    			OR
    			counterparty.name  LIKE (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, CONCAT('%',@SearchString2,'%'), counterparty.name))
    			OR
    			delivery_points.id  = (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, @SearchString2, delivery_points.id))
    			OR
    			counterparty.id = (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, @SearchString2, counterparty.id))
    		)
			AND
			(
    			delivery_points.compiled_address LIKE (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, CONCAT('%',@SearchString3,'%'), delivery_points.compiled_address))
    			OR
    			counterparty.name  LIKE (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, CONCAT('%',@SearchString3,'%'), counterparty.name))
    			OR
    			delivery_points.id  = (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, @SearchString3, delivery_points.id))
    			OR
    			counterparty.id = (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, @SearchString3, counterparty.id))
    		)
			AND
			(
    			delivery_points.compiled_address LIKE (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, CONCAT('%',@SearchString4,'%'), delivery_points.compiled_address))
    			OR
    			counterparty.name  LIKE (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, CONCAT('%',@SearchString4,'%'), counterparty.name))
    			OR
    			delivery_points.id  = (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, @SearchString4, delivery_points.id))
    			OR
    			counterparty.id = (IF(COALESCE(@SearchString1, @SearchString2, @SearchString3, @SearchString4) is not null, @SearchString4, counterparty.id))
    		)
		)
		AND
		(
			@HideWithoutEmail = ''
			OR
			EXISTS (
				SELECT
					id
				FROM
					emails
				WHERE
					emails.counterparty_id = counterparty.id
			)
		)
        
GROUP BY
    delivery_points.id,
    counterparty.id</CommandText>
        <QueryParameters>
          <QueryParameter Name="CounterpartyId">
            <Value>={?CounterpartyId}</Value>
          </QueryParameter>
          <QueryParameter Name="AddressId">
