  <DataSets>
    <DataSet Name="DataIncome">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	account_income.number as num,
 account_income.total as total, account_income.description as description,
 account_income.date as date, cash_income_category.name as category,
 counterparty.full_name as name

FROM account_income
 
LEFT JOIN cash_income_category ON cash_income_category.id = account_income.cash_income_category_id
LEFT JOIN counterparty ON counterparty.id = account_income.counterparty_id
WHERE
account_income.date BETWEEN @startDate AND @endDate 
AND if (@incomeCategory = -1, cash_income_category.id, @incomeCategory) = cash_income_category.id
ORDER BY category;</CommandText>
        <QueryParameters>
          <QueryParameter Name="startDate">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
          <QueryParameter Name="incomeCategory">
            <Value>=Parameters!IncomeCategory</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="num">
          <DataField>num</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="category">
          <DataField>category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="total">
          <DataField>total</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
