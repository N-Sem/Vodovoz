  <DataSets>
    <DataSet Name="Complaint">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    complaints.id,
    complaints.creation_date,
    CASE
        WHEN complaints.status = 'InProcess' THEN 'В работе'
        WHEN complaints.status = 'Checking' THEN 'На проверке'
        WHEN complaints.status = 'WaitingForReaction' THEN 'Ожидает реакции'
        WHEN complaints.status = 'Closed' THEN 'Закрыт'
        END AS complaint_status,
    GROUP_CONCAT(DISTINCT subdivisions.name SEPARATOR ' ') AS subdivisions,
    counterparty.name AS client,
    complaints.arrangement_text AS arrangement_text,
    GROUP_CONCAT(DISTINCT
                 CASE complaint_guilty_items.guilty_type
                     WHEN 'Supplier' THEN 'Поставщик'
                     WHEN 'Client' THEN 'Клиент'
                     WHEN 'None'	THEN 'Нет'
                     WHEN 'Employee' THEN GET_PERSON_NAME_WITH_INITIALS(guiltyEmployee.last_name, guiltyEmployee.name, guiltyEmployee.patronymic)
                     WHEN 'Subdivision' THEN guiltySubdivisions.name
                     END SEPARATOR ';\n')
        AS guilties,
    complaints.complaint_text,
    GROUP_CONCAT(DISTINCT ROUND(fines.total_money,2) SEPARATOR ' ') AS fines,
    complaints.result_text,
    complaints.actual_completion_date,
    DATEDIFF(complaints.actual_completion_date, complaints.creation_date) AS days_at_work,
    complaint_kinds.name as complaint_kind_text,
    complaint_objects.name as complaint_object_text,
    cr_counterparty.name as result_of_counterparty,
    cr_employees.name as result_of_employees
FROM
    complaints
        LEFT JOIN complaint_discussions ON complaint_discussions.complaint_id = complaints.id
        LEFT JOIN subdivisions ON subdivisions.id = complaint_discussions.subdivision_id
        LEFT JOIN counterparty ON counterparty.id = complaints.counterparty_id
        LEFT JOIN orders ON orders.id = complaints.order_id
        LEFT JOIN complaint_guilty_items ON complaint_guilty_items.complaint_id = complaints.id
        LEFT JOIN employees AS guiltyEmployee ON guiltyEmployee.id = complaint_guilty_items.employee_id
        LEFT JOIN subdivisions AS guiltySubdivisions ON guiltySubdivisions.id = complaint_guilty_items.subdivision_id
        LEFT JOIN employees ON employees.id = complaints.created_by_id
        LEFT JOIN complaint_fines ON complaint_fines.complaint_id = complaints.id
        LEFT JOIN fines ON complaint_fines.fine_id = fines.id
        LEFT JOIN complaint_kinds ON complaints.complaint_kind_id = complaint_kinds.id
        LEFT JOIN complaint_objects ON complaint_kinds.complaint_object_id = complaint_objects.id
        LEFT JOIN complaint_results cr_counterparty on complaints.complaint_result_of_counterparty_id = cr_counterparty.id
        LEFT JOIN complaint_results cr_employees on complaints.complaint_result_of_employees_id = cr_employees.id
WHERE
    (
            (
                        @subdivision_id = 0 OR (@subdivision_id = 1
                    AND (@start_date = '' OR complaints.planned_completion_date &gt;= @start_date)
                    AND (@end_date = '' OR complaints.planned_completion_date &lt;= @end_date))
                )
            OR EXISTS
                (
                    SELECT
                        *
                    FROM
                        complaint_discussions as disc
                    WHERE
                            disc.complaint_id = complaints.id
                      AND disc.subdivision_id = @subdivision_id
                      AND (@start_date = '' OR disc.planned_completion_date &gt;= @start_date)
                      AND (@end_date = '' OR disc.planned_completion_date &lt;= @end_date)
                )
        )
  AND (@employee_id = 0 OR complaints.created_by_id = @employee_id)
  AND (@status = '' OR complaints.status = @status)
  AND (@type = '' OR complaints.type = @type)
  AND (@guilty_type = '' OR complaint_guilty_items.guilty_type = @guilty_type)
  AND (@complaint_kind = '' OR complaint_kinds.name = @complaint_kind)
  AND
    (@start_date = ''
        OR (@date_type = 'CreationDate' AND DATE(complaints.creation_date) &lt;= Date(@end_date) AND DATE(complaints.creation_date) &gt;= DATE(@start_date))
        OR (@date_type = 'PlannedCompletionDate' AND DATE(complaints.planned_completion_date) &lt;= DATE(@end_date) AND DATE(complaints.planned_completion_date) &gt;= DATE(@start_date)))
GROUP BY
    complaints.id
ORDER BY complaints.id DESC;</CommandText>
